<Window x:Class="TDC_Extractor.ShortNameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TDC_Extractor"
        mc:Ignorable="d"
        Title="ShortNameDialog" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="Long Name:"/>
            <TextBlock x:Name="LongNameTextBlock"/>
            <Label x:Name="ShortNameLabel" Content="Short Name:"/>
            <ListBox x:Name="ShortNameListBox" MouseDoubleClick="ShortNameListBox_MouseDoubleClick" />
            <Label x:Name="CustomLabel" Content="Custom:"/>
            <TextBox x:Name="CustomTextBox" Text=""/>
            <Button IsDefault="True" Content="OK" Click="Button_Click" />
        </StackPanel>
    </Grid>
</Window>
